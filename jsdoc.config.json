{
  "recurseDepth": 10,
  "source": {
      "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "plugins": [
    "jsdoc-plugin-typescript-new"
  ],
  "typescript": {
    "moduleRoot": "src"
  }
}
